<nav role="navigation">
<ul>
<%= if @current_user do %>
<li><%= @current_user.username %></li>
<li><%= link "My Videos", to: Routes.video_path(@conn, :index) %></li>
<li>
<%= link "Log out",
to: Routes.session_path(@conn, :delete, @current_user),
method: "delete" %>
</li>
<% else %>
<li><%= link "Register", to: Routes.users_path(@conn, :new) %></li>
<li><%= link "Log in", to: Routes.session_path(@conn, :new) %></li>
<% end %>
</ul>
</nav>
<a href="http://phoenixframework.org/" class="phx-logo">
<img src="<%= Routes.static_url(@conn, "/images/phoenix.png") %>"
alt="Phoenix Framework Logo"/>
</a>

      </section>
    </header>
    <main role="main" class="container">
      <p class="alert alert-info" role="alert"><%= get_flash(@conn, :info) %></p>
      <p class="alert alert-danger" role="alert"><%= get_flash(@conn, :error) %></p>
      [inner start]
      <%= @inner_content %>
      [inner end]
    </main>
  </body>
